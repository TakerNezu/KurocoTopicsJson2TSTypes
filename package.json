{
  "name": "KurocoTopicsJson2TSTypes",
  "version": "0.1.0",
  "description": "Convert a JSON file of Kuroco Topics to TypeScript Types.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/TakerNezu/KurocoTopicsJson2TSTypes.git",
  "author": "TakerNezu",
  "license": "MIT",
  "dependencies": {
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "ts-jest": {
            "tsConfig": "tsconfig.json"
          }
        }
      ]
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest"
  }
}
